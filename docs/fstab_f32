/dev/mapper/fedora_localhost--live-root /       ext4    defaults        1 1
UUID=66398534-1392-435e-b340-e638aec0effa /boot ext4    defaults        1 2
UUID=FB14-6CD9          /boot/efi               vfat    umask=0077,shortname=winnt 0 2
/dev/mapper/fedora_localhost--live-home /home	ext4    defaults        1 2
/dev/mapper/fedora_localhost--live-swap none	swap    defaults        0 0
/dev/mapper/fedora00-home /homx	ext4    defaults        1 2
/dev/mapper/fedora00-root /mntx	ext4    defaults        1 2
/dev/sdc1	/BACKUP	auto	defaults	1	2
/dev/sde1	/tv	ntfs	user,exec,uid=swang,gid=swang	0	2
